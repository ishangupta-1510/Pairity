{
  "name": "pairity",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prepare": "husky",
    "setup:env": "node scripts/setup-env.js",
    "postinstall": "node scripts/setup-env.js",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,yml,yaml}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,yml,yaml}\"",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf node_modules .expo dist",
    "clean:cache": "expo start --clear",
    "reset": "npm run clean && npm install",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "validate": "npm run lint && npm run format:check && npm run typecheck",
    "pre-commit": "lint-staged",
    "commit": "cz",
    "prompts": "node scripts/auto-prompt-processor.js",
    "prompts:status": "node scripts/auto-prompt-processor.js --status",
    "prompts:reset": "node scripts/auto-prompt-processor.js --reset",
    "prompts:clear-blocked": "node scripts/auto-prompt-processor.js --clear-blocked",
    "prompts:test": "node scripts/test-setup.js"
  },
  "dependencies": {
    "expo": "~49.0.0",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "react-native-screens": "~3.22.0",
    "react-native-safe-area-context": "4.6.3",
    "expo-linear-gradient": "~12.3.0",
    "expo-blur": "~12.4.1",
    "expo-haptics": "~12.4.0",
    "react-native-reanimated": "~3.3.0",
    "react-native-gesture-handler": "~2.12.0",
    "@react-native-async-storage/async-storage": "1.18.2",
    "react-hook-form": "^7.48.2",
    "yup": "^1.3.3",
    "@hookform/resolvers": "^3.3.2",
    "react-native-vector-icons": "^10.0.3",
    "expo-font": "~11.4.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "@reduxjs/toolkit": "^2.0.1",
    "react-redux": "^9.0.4",
    "redux-persist": "^6.0.0",
    "axios": "^1.6.2",
    "react-native-toast-message": "^2.1.7",
    "react-native-elements": "^3.4.3",
    "react-native-paper": "^5.11.3",
    "react-native-mmkv": "^2.11.0",
    "socket.io-client": "^4.5.4",
    "date-fns": "^2.30.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "@types/react-native": "^0.72.0",
    "@types/lodash": "^4.14.202",
    "typescript": "^5.1.3"
  },
  "private": true
}
